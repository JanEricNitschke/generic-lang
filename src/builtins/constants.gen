# Builtin utility functions

# Check if all elements in an iterable are truthy
fun all(iterable) {
    foreach (var item in iterable) {
        if (!item) {
            return false;
        }
    }
    return true;
}

# Check if any element in an iterable is truthy
fun any(iterable) {
    foreach (var item in iterable) {
        if (item) {
            return true;
        }
    }
    return false;
}