var name = "World";
print("Hello ${name}"); # expect: Hello World

var age = 25;
print("I am ${age} years old"); # expect: I am 25 years old

var x = 3.14;
print("Pi is approximately ${x}"); # expect: Pi is approximately 3.14

print("${name} is ${age}"); # expect: World is 25

print("Multiple ${age} + ${x} = ${age + x}"); # expect: Multiple 25 + 3.14 = ${age + x}