var name = "Bob";
print(f"Hi, I'm ${name}."); # expect: Hi, I'm Bob.

# Basic string parts
print(f"Hello world"); # expect: Hello world

# Multiple interpolations
var first = "John";
var last = "Doe";
print(f"Name: ${first} ${last}"); # expect: Name: John Doe

# Interpolation at start and end
var greeting = "Hello";
var punctuation = "!";
print(f"${greeting}, world${punctuation}"); # expect: Hello, world!

# Single interpolation
var value = "test";
print(f"${value}"); # expect: test