var name = "Bob";
print f"Hi, I'm ${name}."; # expect: Hi, I'm Bob.

var age = 25;
print f"I am ${age} years old."; # expect: I am 25 years old.

# Basic string parts
print f"Hello world"; # expect: Hello world

# Empty interpolation
var empty = "";
print f"Value: ${empty}End"; # expect: Value: End

# Multiple interpolations
var first = "John";
var last = "Doe";
print f"Name: ${first} ${last}"; # expect: Name: John Doe