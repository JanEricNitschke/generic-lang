class Error < Exception {}

{
    var a = 1;
    try {
        var b = 2;
        var c = 3;
        print(b); # expect: 2
        print("Try"); # expect: Try
    } catch Error {
        print("Catch");
    } else {
        var d = 4;
        print(d); # expect: 4
        print(a); # expect: 1
        print("Else"); # expect: Else
    }
    var e = 5;
    print("Done"); # expect: Done
