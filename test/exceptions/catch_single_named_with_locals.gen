class Error {}
{
    var a = 1;
    try {
        var b = 2;
        throw Error();
        var c = 3;
        print("Try");
    } catch Error as e {
        var d = 4;
        print(d); # expect: 4
        print(e); # expect: <Error instance>
    } else {
        print("Else");
    }
    print("Done"); # expect: Done
}