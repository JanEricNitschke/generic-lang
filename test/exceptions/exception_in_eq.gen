class Error < Exception {}

class ThrowOnEq {
    __eq__() {
        throw Error();
    }
}

try {
    ThrowOnEq() == ThrowOnEq()
} catch Error {
    print("Caught");  # expect: Caught
} else {
    print("No Exception");
}

