class A {
  __init__(param) {
    this.field = param;
  }

  test() {
    print(this.field);
  }
}

class B < A {}

var b = B("value");
b.test(); # expect: value

class D < List {
  custom_method() {
    print("working with inherited list");
  }
}

var d = D();
d.append(42);
print(len(d)); # expect: 1
d.custom_method(); # expect: working with inherited list
