# Test Bool constructor with various inputs

# Basic boolean values
print(Bool(true)); # expect: true
print(Bool(false)); # expect: false

# Falsey values
print(Bool(nil)); # expect: false
print(Bool(false)); # expect: false

# Truthy values
print(Bool(0)); # expect: true
print(Bool(1)); # expect: true
print(Bool(-1)); # expect: true
print(Bool(3.14)); # expect: true
print(Bool("")); # expect: true
print(Bool("hello")); # expect: true
print(Bool("false")); # expect: true

# No arguments (should default to nil, which is falsey)
print(Bool()); # expect: false