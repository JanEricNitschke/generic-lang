# Test Float constructor with various inputs

# String parsing
print(Float("3.14")); # expect: 3.14
print(Float("-2.5")); # expect: -2.5
print(Float("0.0")); # expect: 0.0
print(Float("42")); # expect: 42.0

# From numbers
print(Float(42)); # expect: 42.0
print(Float(3.14)); # expect: 3.14

# From booleans
print(Float(true)); # expect: 1.0
print(Float(false)); # expect: 0.0