# Test Integer constructor with various inputs

# String parsing
print(Integer("123")); # expect: 123
print(Integer("-456")); # expect: -456
print(Integer("0")); # expect: 0

# From numbers
print(Integer(42)); # expect: 42
print(Integer(3.14)); # expect: 3
print(Integer(3.99)); # expect: 3
print(Integer(-2.7)); # expect: -2

# From booleans
print(Integer(true)); # expect: 1
print(Integer(false)); # expect: 0