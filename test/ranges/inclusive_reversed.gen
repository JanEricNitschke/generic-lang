var a = 10..=5;
print(a);  # expect: 10..<4

foreach (var x in a) {
    print(x);  # expect: 10
               # expect: 9
               # expect: 8
               # expect: 7
               # expect: 6
               # expect: 5
}

var b = -1..=-5;
print(b);  # expect: -1..<-6

foreach (var x in b) {
    print(x);  # expect: -1
               # expect: -2
               # expect: -3
               # expect: -4
               # expect: -5
}

var c = 5..=5;
print(c);  # expect: 5..<6

foreach (var x in c) {
    print(x);  # expect: 5
}