# Test comparison operators
class TestNumber {
    __init__(value) {
        this.value = value;
    }

    __lt__(other) {
        return this.value < other.value;
    }

    __le__(other) {
        return this.value <= other.value;
    }

    __gt__(other) {
        return this.value > other.value;
    }

    __ge__(other) {
        return this.value >= other.value;
    }
}

var a = TestNumber(5);
var b = TestNumber(3);
var c = TestNumber(5);

print("=== Comparison Operators ===");

print("5 < 3: " + str(a < b)); # expect: 5 < 3: false
print("3 < 5: " + str(b < a)); # expect: 3 < 5: true

print("5 <= 3: " + str(a <= b)); # expect: 5 <= 3: false
print("5 <= 5: " + str(a <= c)); # expect: 5 <= 5: true

print("5 > 3: " + str(a > b)); # expect: 5 > 3: true
print("3 > 5: " + str(b > a)); # expect: 3 > 5: false

print("5 >= 3: " + str(a >= b)); # expect: 5 >= 3: true
print("5 >= 5: " + str(a >= c)); # expect: 5 >= 5: true