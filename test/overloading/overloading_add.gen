class Vector {
    __init__(x, y) {
        this.x = x;
        this.y = y;
    }

    __add__(other) {
        return Vector(this.x + other.x, this.y + other.y);
    }
}

var v1 = Vector(1, 2);
var v2 = Vector(3, 4);
var v3 = v1 + v2;
print(v3.x); # expect: 4
print(v3.y); # expect: 6

# Test that regular number addition still works
print(1 + 2); # expect: 3

# Test that string addition still works  
print("hello" + " world"); # expect: hello world